{
  "name": "hyperf/single-flight-incubator",
  "description": "A concurrent library for Hyperf",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "single-flight",
    "barrier",
    "semephore",
    "worker-pool"
  ],
  "homepage": "https://github.com/hyperf/single-flight-incubator",
  "support": {
    "issues": "https://github.com/hyperf/single-flight-incubator/issues",
    "source": "https://github.com/hyperf/single-flight-incubator"
  },
  "autoload": {
    "psr-4": {
      "Hyperf\\Incubator\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/di": "~3.1.0",
    "hyperf/support": "~3.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\Incubator\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    },
    "hyperf": {
      "config": "Hyperf\\Incubator\\ConfigProvider"
    }
  }
}
